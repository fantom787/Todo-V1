<%- include("header") -%>
    <div class="box" id="heading">
        <h1>
            <%=typeOfDay%>
        </h1>
    </div>
    <div class="box">
        <% for(var i=0;i<newListItem.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%=newListItem[i]%>
                </p>
            </div>
            <% } %>
                <form action="/" class="item" method="post">
                    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="list" value="<%=val%>">+</button>
                </form>
    </div>
    <%- include("footer") -%>
